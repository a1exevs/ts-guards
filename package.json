    "test:watch": "jest --config jest.config.ts --watch"
  },
  "devDependencies": {
    "@types/jest": "29.5.14",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "lint-staged": "15.3.0",
    "path": "0.12.7",
    "prettier": "3.4.2",
    "rimraf": "6.0.1",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "typescript": "5.7.2"
  }
